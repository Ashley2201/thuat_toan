import math
import random
import array
def SNT(n):
    if(n < 2): return False
    if n < 4: return True
    for i in range(2, int(math.sqrt(n))+1):
        if n % i == 0: return False
    return True
N = int(input("Nhap n: "))
array = [random.randint(1, 100) for _ in range(N)]
for i in range(0, len(array)):
    if SNT(array[i]) == True: print(array[i])