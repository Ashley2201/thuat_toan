import math

def SNT(n):
    if(n < 2): return False
    if(n < 4): return True
    for i in range(2, int(math.sqrt(n))+1):
        if(n % i == 0): return False
    return True

def tongNT():
    tong = 0
    n = int(input("Nhap n:"))
    for i in range(2, n):
        if(SNT(i) == True): tong+=i
    print(tong)

tongNT()