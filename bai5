import math

def SNT(n):
    if(n < 2): return False
    if(n < 4): return True
    for i in range(2, int(math.sqrt(n)) + 1):
        if(n % i == 0): return False
    return True

def tongSNT():
    a = int(input("Nhap a: "))
    b = int(input("Nhap b: "))
    tong = 0
    for i in range(a+1, b):
        if(SNT(i) == True): tong+=i
    print(tong)

if(__name__ == "__main__"):
    tongSNT()