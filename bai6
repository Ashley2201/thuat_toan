import math

def tongUoc(n):
    tong = 0
    for i in range(1, int(n/2) + 1):
        if(n % i == 0): tong+=i
    return tong

def thanThiet():
    n = int(input("Nhap n: "))
    for i in range(1, n):
        for j in range(i + 1, n):
            if(tongUoc(i) == j) and (tongUoc(j) == i):
                print(i, j, "\n")

if(__name__ == "__main__"):
    thanThiet()