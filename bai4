import math

def SNT(n):
    if(n < 2): return False
    if(n < 4): return True
    for i in range(2, int(math.sqrt(n)) + 1):
        if(n % i == 0): return False
    return True
def demSNT(a, b):
    dem = 0
    for i in range(a + 1, b):
        if(SNT(i) == True): dem +=1
    print(dem)

if(__name__ == "__main__"):
    a = int(input("Nhap a: "))
    b = int(input("Nhap b: "))
    demSNT(a, b)