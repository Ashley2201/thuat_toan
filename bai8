import math

def baUoc(n):
    dem = 0
    for i in range(1, int(n/2) + 1):
        if(n % i == 0): dem +=1
        if(dem > 2): return False
    if(dem == 2): return True

def tPrime():
    n = int(input("Nhap n: "))
    for i in range(2, n):
        if(baUoc(i) == True): print(i)

if(__name__ == "__main__"):
    tPrime()