import math

def SNT(n):
    if n < 2: return False
    if n < 4: return True
    for i in range(2, int(math.sqrt(n))+1):
        if n % i == 0: return False
    return True

A = int(input("Nhap A: "))
B = int(input("Nhap B: "))
C = int(input("Nhap C: "))

x = 1

p = A * x * x + B * x + C
while SNT(p) != True:
    x+=1
    p = A * x * x + B * x + C

if SNT(p) == True: print(x)